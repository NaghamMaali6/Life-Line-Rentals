      <ImageView fx:id="Employee" fitHeight="152.0" fitWidth="173.0" layoutX="253.0" layoutY="370.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Invoice" fitHeight="152.0" fitWidth="173.0" layoutX="836.0" layoutY="128.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Supplier" fitHeight="152.0" fitWidth="173.0" layoutX="445.0" layoutY="370.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Maintenance_Request" fitHeight="152.0" fitWidth="173.0" layoutX="641.0" layoutY="371.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Workshop" fitHeight="152.0" fitWidth="173.0" layoutX="837.0" layoutY="371.0" pickOnBounds="true" preserveRatio="true" />
      
      <!-- Logout Button and image: -->
      <Button fx:id="LogoutB" layoutX="471.0" layoutY="618.0" mnemonicParsing="false" onAction="#btnlog_out" prefHeight="30.0" prefWidth="122.0" style="-fx-background-color: #5a99a1;" text="Log-out!" textFill="WHITE" underline="true">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />  <!-- Styling for the text -->
         </font>
      </Button>
      <ImageView fx:id="Logout" fitHeight="71.0" fitWidth="92.0" layoutX="385.0" layoutY="598.0" pickOnBounds="true" preserveRatio="true" />
      
   </children>
</AnchorPane>

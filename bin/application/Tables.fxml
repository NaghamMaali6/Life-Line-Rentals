            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="Maintenance_RequestB" layoutX="647.0" layoutY="526.0" mnemonicParsing="false" style="-fx-background-color: #5a99a1;" text="Maintenance_Request" textFill="WHITE" onAction="#btnMaintenance_Request">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="WorkshopB" layoutX="876.0" layoutY="527.0" mnemonicParsing="false" style="-fx-background-color: #5a99a1;" text="Workshop" textFill="WHITE" onAction="#btnWorkshop">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      
      <!-- Images for each table: -->
      <ImageView fx:id="Customer" fitHeight="152.0" fitWidth="173.0" layoutX="20.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Equipment" fitHeight="152.0" fitWidth="173.0" layoutX="245.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="Rental" fitHeight="152.0" fitWidth="173.0" layoutX="450.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true" />
